{
  "name": "electron-demo",
  "private": true,
  "version": "1.1.0",
  "main": "./electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron": "electron .",
    "electron-prod": "electron .",
    "build:electron": "vite build && electron-builder",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.example.app",
    "asar": false,
    "files": [
      "dist/**/*",
      "electron/**/*",
      "package.json"
    ],
    "directories": {
      "output": "build"
    },
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "bonjs",
        "repo": "test-demo"
      }
    ]
  },
  "dependencies": {
    "axios": "^1.8.2",
    "electron-log": "^5.3.2",
    "electron-updater": "^6.3.9",
    "element-plus": "^2.9.6",
    "node-fetch": "^3.3.2",
    "node-printer": "^1.0.4",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "electron": "^35.0.1",
    "electron-builder": "^25.1.8",
    "electron-rebuild": "^3.2.9",
    "vite": "^6.2.0"
  }
}
